{
  "name": "lyticus",
  "description": "Tracker library for Lyticus",
  "private": false,
  "version": "1.0.38",
  "author": "Bjorn Krols (https://byteboomers.com)",
  "license": "MIT",
  "keywords": [
    "analytics",
    "lyticus",
    "tracking"
  ],
  "homepage": "https://github.com/byteboomers/lyticus-tracker#readme",
  "repository": {
    "type": "git",
    "url": "git+https://github.com/byteboomers/lyticus-tracker.git"
  },
  "bugs": {
    "url": "https://github.com/byteboomers/lyticus-tracker/issues"
  },
  "main": "dist/index.js",
  "types": "dist/index.d.ts",
  "files": [
    "dist",
    "src"
  ],
  "engines": {
    "node": ">=10"
  },
  "scripts": {
    "build": "webpack --mode production --progress",
    "format": "prettier --write --ignore-path .gitignore \"**/*.+(js|jsx|ts|tsx|css|scss|json|md|yml)\""
  },
  "husky": {
    "hooks": {
      "pre-commit": "pretty-quick --staged"
    }
  },
  "dependencies": {
    "custom-event-polyfill": "^1.0.7",
    "js-cookie": "^2.2.1",
    "lodash.isobject": "^3.0.2",
    "url-search-params-polyfill": "^8.0.0"
  },
  "devDependencies": {
    "@types/js-cookie": "^2.2.6",
    "@types/lodash.isobject": "^3.0.6",
    "husky": "^4.3.0",
    "prettier": "^2.2.0",
    "pretty-quick": "^3.1.0",
    "react": "^16.13.1",
    "react-dom": "^17.0.1",
    "ts-loader": "^8.0.11",
    "tslib": "^2.0.3",
    "typescript": "^4.0.3",
    "webpack": "^5.6.0",
    "webpack-cli": "^4.2.0"
  }
}
